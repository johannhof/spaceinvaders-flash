package {	import flash.display.MovieClip;	public class Barrier extends MovieClip	{		protected var _elements:Array;		public function Barrier(offsetX:int)		{			_elements = new Array();			for (var w = 0; w < 30; w++)			{				for (var h = 0; h < 15; h++)				{					var temp:BarrierElement = new BarrierElement();					temp.x = 3 * w + offsetX;					temp.y = 3 * h + 370;					_elements.push(temp);					this.addChild(temp);				}			}			for (var hight = 0; hight < 14; hight++)			{				for (var wide = 1; wide < 15; wide++)				{				this.removeChild(_elements[wide*hight]);				}			}		}		public function get elements():Array		{			return _elements;		}	}}